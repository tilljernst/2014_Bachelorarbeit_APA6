	Journal = {Human Performance},
	Number = {3},
	Pages = {253--275},
	Title = {{Working Memory Dimensions as Differential Predictors of the Speed and Error Aspect of Multitasking Performance}},
	Url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=psyc5&AN=2006-09596-004},
	Urldate = {2014-05-11},
