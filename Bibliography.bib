@article{Dreher2003,
	Abstract = {A fundamental question about the nature of cognitive control is whether performing two tasks successively or simultaneously activates distinct brain regions. To investigate this issue, a functional magnetic resonance imaging ({fMRI)} study was designed to compare task-switching and dual-task performance among 8 adults. Results showed that performing two tasks successively or simultaneously activated a common prefronto-parietal neural network relative to performing each task separately. More importantly, the anterior cingulate and the lateral prefrontal cortices were found to be differently activated in dual-task and task-switching situations. When performing two tasks simultaneously, as compared to performing them in succession, activation was found in the rostral anterior cingulate cortex. In contrast, switching between two tasks, relative to performing them simultaneously, activated the left lateral prefrontal cortex and the bilateral intra-parietal sulcus region. Results are interpreted as indicating that the rostral anterior cingulate cortex serves to resolve conflicts between stimulus-response associations when performing two tasks simultaneously, while the lateral prefrontal cortex dynamically selects the neural pathways needed to perform a given task during task switching. ({PsycINFO} Database Record (c) 2013 {APA}, all rights reserved).},
	Author = {Dreher, Jean-Claude and Grafman, Jordan},
	Date-Added = {2014-05-11 08:20:15 +0000},
	Date-Modified = {2014-05-11 08:20:15 +0000},
	Doi = {http://dx.doi.org/10.1093/cercor/13.4.329},
	Issn = {1047-3211},
	Journal = {Cerebral Cortex},
	Number = {4},
	Pages = {329--339},
	Title = {{Dissociating the roles of the rostral anterior cingulate and the lateral prefrontal cortices in performing two tasks simultaneously or successively}},
	Url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=psyc4&AN=2003-99011-001},
	Urldate = {2014-05-11},
